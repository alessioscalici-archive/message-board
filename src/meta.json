      "index": "test/index.html",
      "vendor": {
        "head": {
          "dev": [],
          "prod": []
        },
        "body": {
          "dev": [
            "./build/vendor/angular/angular.js",
            "./build/vendor/angular-ui-router/release/angular-ui-router.js",
            "./build/vendor/ngstorage/ngStorage.js"
          ],
          "prod": [
            "./build/vendor/angular/angular.min.js",
            "./build/vendor/angular-ui-router/release/angular-ui-router.min.js",
            "./build/vendor/ngstorage/ngStorage.min.js"
          ]
        }
      }
    }
  ]
}